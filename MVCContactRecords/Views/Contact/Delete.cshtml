@model Contact
<h1>Delete Supplier</h1>
<div>
    <h4>Are you sure you want to Delete this Supplier</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">First Name</dt>
        <dd class="col-sm-10">@Model.FirstName</dd>
        <dt></dt>
        <dt class="col-sm-2">Last Name</dt>
        <dd class="col-sm-10">@Model.LastName</dd>
        <dt></dt>
        <dt class="col-sm-2">Contact Number</dt>
        <dd class="col-sm-10">@Model.ContactNumber</dd>
        <dt></dt>
        <dt class="col-sm-2">Email</dt>
        <dd class="col-sm-10">@Model.Email</dd>
        <dt></dt>
        <dt class="col-sm-2">Contact Description</dt>
        <dd class="col-sm-10">@Model.ContactDescription</dd>
        <dt></dt>
    </dl>
    <form asp-action="DeleteConfirmed" method="post">
        <input type="hidden" asp-for="ContactId" />
        <div class="form-group">
            <input type="submit" value="Delete" class="btn btn-danger" />
            <a asp-action="Index1">Back to List</a>
        </div>
    </form>
</div>